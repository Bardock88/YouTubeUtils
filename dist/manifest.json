{
  "manifest_version": 2,
  "name": "YouTubeUtils",
  "version": "1.0",
  "description": "Extra YouTube functionality",
  "icons": {
    "16": "./icons/icon-16.png",
    "48": "./icons/icon-48.png",
    "128": "./icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/watch*"
      ],
      "js": [
        "js/main.js"
      ]
    }
  ]
}
